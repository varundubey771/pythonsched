{"buildCommand": "prisma generate && prisma migrate deploy && next build",
	    "builds": [{
	        "src": "main.py",
	        "use": "@vercel/python",
	        "config": { "maxLambdaSize": "15mb" }
	    }],
	    "routes": [
	        {
	            "src": "/(.*)",
	            "dest": "main.py"
	        }
	    ]
}
